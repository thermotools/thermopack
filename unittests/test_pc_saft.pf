    real :: rho,T,x(2)

    call init_thermo(eosLibrary='THERMOPACK',alpha='Classic',eos='PC-SAFT',mixing='Classic',ncomp=2,&
         comp_string="C1,H2O",nphases=2,saft_setno=(/1,1/))
    rho = 4/5e-2
    T = 300
    x = (/2.0,1.2/)


    call pc_saft_set_kij_asym(1,2,0.01)
    call pc_saft_set_kij_asym(2,1,-0.04)


    ! Verbose routines. Uncomment for thorough testing.
    ! call test_n_der(x,a_i)
    ! call test_Tn_der(T,x,m2e1s3_mean)
    ! call test_Tn_der(T,x,m2e2s3_mean)

    call test_1st_der(eta,rho,T,x)
    call test_1st_der_dim(zeta,rho,T,x,dim=4)
