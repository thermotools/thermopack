    real :: T0, P0, n0(ncomp)
    n0 = [0.9,0.15,0.15]
    P0 = 1.0e6
    T0 = 230.0

    if (VERBOSE) write(*,'(4A)') "TESTING ", eos, " FOR ", compstr
    call test_fres_tv_model(ncomp,eos,compstr,T0,P0,n0)
    call test_consistency(ncomp,eos,compstr,T0,P0,n0)
  end subroutine test_fres_tv_CSP_SRK


