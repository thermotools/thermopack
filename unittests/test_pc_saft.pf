    call cleanup_eos()
  end subroutine test_der

  @Test
  subroutine test_pure_density_accuracy
    real :: V, P, x(1)
    real :: rhoLiq, rhoVap
    integer :: nc

    nc = 1
    x = (/1.0/)

    ! Should estimate the liquid and vapor density of propane to within 5 %.
    call init_thermo(eosLibrary='THERMOPACK',alpha='Classic',eos='PC-SAFT',&
         mixing='Classic',ncomp=1,comp_string="C3",nphases=2,saft_setno=(/1/))
    P = 101325.0

    call saft_master_volume_solver(nc,comp,cbeos(1),T=298.0,P_spec=P,n=x,phase=LIQPH,V=V)
    rhoLiq = (3*12+8*1.01)*1e-3/V
    @assertEqual(rhoLiq, 493.0, 493.0*0.05)
    call saft_master_volume_solver(nc,comp,cbeos(1),T=273.0,P_spec=P,n=x,phase=VAPPH,V=V)
    rhoVap = (3*12+8*1.01)*1e-3/V
    @assertEqual(rhoVap, 2.0098, 2.0098*0.05)
    call cleanup_eos()

    ! Should estimate the liquid and vapor density of water to within 10 %.
    call init_thermo(eosLibrary='THERMOPACK',alpha='Classic',eos='PC-SAFT',&
         mixing='Classic',ncomp=1,comp_string="H2O",nphases=2,saft_setno=(/1/))
    P = 101325.0

    call saft_master_volume_solver(nc,comp,cbeos(1),T=277.0,P_spec=P,n=x,phase=LIQPH,V=V)
    rhoLiq = 18.02*1e-3/V
    @assertEqual(rhoLiq, 1000.0, 1000.0*0.1)
    call saft_master_volume_solver(nc,comp,cbeos(1),T=298.0,P_spec=P,n=x,phase=VAPPH,V=V)
    rhoVap = 18.02*1e-3/V
    @assertEqual(rhoVap, 0.804, 0.804*0.1)

    call cleanup_eos()
  end subroutine test_pure_density_accuracy

  @Test
  subroutine test_mixture_density_accuracy
    real :: V, T, P, x(3)
    real :: rhoVap
    integer :: nc

    nc = 3
    x = (/0.78,0.21,0.01/)
    T = 273.15
    P = 1e5

    ! Should estimate density of air at STP to within 1 %.
    call init_thermo(eosLibrary='THERMOPACK',alpha='Classic',eos='PC-SAFT',&
         mixing='Classic',ncomp=nc,comp_string="N2,O2,AR",nphases=2,saft_setno=(/1,1,1/))


    call saft_master_volume_solver(nc,comp,cbeos(1),T=T,P_spec=P,n=x,phase=VAPPH,V=V)
    rhoVap = (28.9645)*1e-3/V
    @assertEqual(1.2754, rhoVap, 1.2754*0.01)


    call cleanup_eos()
  end subroutine test_mixture_density_accuracy

end module test_pc_saft
